{
  "name": "power-grid-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t power-grid-builder:latest .",
    "docker:run": "docker run -d --name power-grid-builder -p 8080:80 --restart unless-stopped power-grid-builder:latest",
    "docker:stop": "docker stop power-grid-builder && docker rm power-grid-builder",
    "docker:logs": "docker logs power-grid-builder",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}
